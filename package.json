{
  "name": "modern-js-snippets",
  "displayName": "Modern JavaScript Snippets ⚡",
  "version": "0.2.0",
  "license": "MIT",
  "description": "Code snippets for modern JavaScript & TypeScript",
  "icon": "assets/icon.png",
  "bugs": "https://github.com/mat2ja/modern-javascript-snippets/issues",
  "author": {
    "name": "Matija Osrečki",
    "email": "matija.osrecki@gmail.com"
  },
  "publisher": "matijao",
  "repository": "https://github.com/mat2ja/modern-javascript-snippets",
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "javascript",
    "typescript",
    "js",
    "ts",
    "es6",
    "es10",
    "esnext",
    "intellisense",
    "autocomplete",
    "vscode",
    "snippets",
    "snippet"
  ],
  "galleryBanner": {
    "color": "#FED703",
    "theme": "light"
  },
  "engines": {
    "vscode": "^1.x.x"
  },
  "scripts": {
    "publish": "vsce package && vsce publish",
    "generate": "deno run --allow-write --allow-read src/app.ts",
    "generate:table": "deno run --allow-write --allow-read src/app.ts --table --snippets=false",
    "generate:all": "deno run --allow-write --allow-read src/app.ts --table --snippets"
  },
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./dist/js.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./dist/js.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./dist/js.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./dist/js.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./dist/ts.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./dist/ts.code-snippets"
      }
    ]
  }
}
